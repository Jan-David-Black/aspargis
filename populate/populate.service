[Unit]
Description=Write COAP messages to Hasura Graphql API

[Service]
ExecStart=/home/ubuntu/aspargis/populate/coap.js
Restart=always
User=ubuntu
# Note Debian/Ubuntu uses 'nogroup', RHEL/Fedora uses 'nobody'
Group=nogroup
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
